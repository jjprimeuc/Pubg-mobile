            "value":"12;9"
        },
        "AUDIO_DATA": {
            "description": "audio package name",
            "required": true,
            "value":"JEOL-xPRIME;PUBG;https://cdn.ironman.my.id/q/gHRsu.jpg"
        },
        "WARN_COUNT": {
            "description": "warn count",
            "required": true,
            "value":"2"
        },
        "MEDIA_DATA": {
            "description": "media data that uses in list",
            "required": true,
            "value":" ü…™Íú±·¥õ ·¥ç·¥á…¥·¥ú;PUBG-·¥ç·¥Ö;https://cdn.ironman.my.id/q/gHRsu.jpg"
        },
        "HANDLER": {
            "description": "handler for bot. all so can be null or false",
            "required": true,
            "value":"null"
        },
        "SESSION_ID": {
            "description": "session id",
            "required": true
        },
        "WORK_TYPE": {
            "description": "worker type public or private",
            "required": true,
            "value":"private"
        },
        "HEROKU_APP_NAME": {
            "description": "heroku app name, same as above entered",
            "required": true
        },
        "HEROKU_API_KEY": {
            "description": "Heroku account api key, https://dashboard.heroku.com/account",
            "required": true
        },
        "STICKER_PACKNAME": {
            "description": "sticker package name",
            "required": true,
            "value":"2025;PUBG"
        },
        "SUDO": {
            "description": "owner number of the bot",
