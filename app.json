    "success_url": "/",
    "stack": "container",
    "env": {
        "BOT_INFO": {
            "description": "botname;ownername;image url",
            "required": true,
            "value":"PUBG;jOEL-xMD;https://cdn.ironman.my.id/q/gHRsu.jpg"
        },
        "MENU_FONT": {
