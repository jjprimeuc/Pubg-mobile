        "STATUS_REPLY": {
            "description": "Reply to status",
            "required": false,
            "value":"false"
        },
        "STATUS_REPLY_MSG": {
            "description": "Status reply message to send",
            "required": false,
            "value":"*Up for a match mate ü§†‚ú®*"
        },
        "CMD_REACTION": {
            "description": "react to commands",
            "required": false,
            "value":"false"
        }
    },
      "addons": [{ "plan": "heroku-postgresql" }],
      "buildpacks": [
                    { "url": "https://github.com/heroku/heroku-buildpack-nodejs#latest" },
